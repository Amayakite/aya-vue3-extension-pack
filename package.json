{
    "name": "aya-vue3-extension-pack",
    "displayName": "Aya Vue3 extension Pack",
    "publisher": "Amayakite",
    "description": "A collection of extensions for working with Vue3/Volar Applications in VS Code",
    "license": "MIT",
    "version": "0.0.8",
    "engines": {
        "vscode": "^1.68.0"
    },
    "keywords": [
        "vue",
        "vue3",
        "volar",
        "aya",
        "extension pack"
    ],
    "categories": [
        "Extension Packs",
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/amayakite/aya-vue3-extension-pack"
    },
    "homepage": "https://github.com/amayakite/aya-vue3-extension-pack",
    "icon": "vuejs-logo.png",
    "extensionPack": [
        "Vue.volar",
        "Vue.vscode-typescript-vue-plugin",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "sibiraj-s.vscode-scss-formatter",
        "formulahendry.auto-rename-tag",
        "formulahendry.auto-close-tag",
        "christian-kohler.path-intellisense",
        "mikestead.dotenv",
        "steoates.autoimport",
        "xabikos.javascriptsnippets",
        "bradlc.vscode-tailwindcss",
        "hollowtree.vue-snippets",
        "usernamehw.errorlens",
        "Gruntfuggly.todo-tree",
        "pflannery.vscode-versionlens",
        "wix.vscode-import-cost"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "vue",
                "path": "./snippets/vue.json"
            }
        ]
    }
}